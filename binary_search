def linear_search(n,list,key):
    for i in range(0,n):
        if list[i]==key:
            return i  
    return -1    

list=[x for x in range(1,20,3)]
print(list)
key=int(input("Enter the key:"))
n=len(list)
res=linear_search(n,list,key)

if res==-1:
    print('element not found')
else:
    print('element found at',res)
    
